{
  "name": "pvweb",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:standalone": "nuxt build --standalone",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js-ci": "eslint --ext \".js,.vue\" --max-warnings 0 --ignore-path .gitignore -f json -o report/eslint.json .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"**/*.{vue, scss,css}\" --ignore-path .gitignore",
    "lint:style-ci": "stylelint \"**/*.{vue,scss,css}\" --max-warnings 0 --ignore-path .gitignore -f json -o report/stylelint.json",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:ci": "npm run lint && npm run lint:js-ci && npm run lint:style-ci",
    "lsg": "nuxt storybook -s .storybook/static",
    "lsg:build": "nuxt storybook build -s .storybook/static --output-dir ./lsg-public",
    "test": "jest",
    "storyblok:sync": "env-cmd node -r esm ./bin/nuxt-storyblok.js"
  },
  "dependencies": {
    "@nuxt/ufo": "^0.5.4",
    "@nuxtjs/composition-api": "^0.31.0",
    "@nuxtjs/storybook": "^4.2.0",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "@txp-cms/storyblok": "^1.6.5",
    "core-js": "^3.21.1",
    "env-cmd": "^10.1.0",
    "get-youtube-id": "^1.0.1",
    "material-design-icons-iconfont": "^6.1.1",
    "module-alias": "^2.2.2",
    "nanoid": "^3.3.0",
    "nuxt": "^2.14.12",
    "storyblok-js-client": "^4.2.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "ufo": "^0.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.4.15",
    "@storybook/addon-backgrounds": "^6.0.21",
    "@storybook/addon-controls": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/addons": "^6.4.9",
    "@storybook/theming": "^6.4.9",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^27.5.1",
    "jest-fail-on-console": "^2.3.0",
    "jsdom-global": "^3.0.2",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "sass-loader": "10.1.1",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
