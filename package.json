{
  "name": "pvacviewer",
  "version": "2.0.0",
  "description": "Frontend application for Pfeiffer Vacuum Homepage",
  "author": "diva-e",
  "private": true,
  "scripts": {
    "dev": "nodemon server/index.js",
    "build": "nuxt build",
    "start": "node server/index.js",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "cms:export": "env-cmd -f ./cms/storyblok.env storyblok-component-export",
    "cms:migrate-components": "env-cmd -f ./cms/storyblok.env storyblok-migrate --component-migrations",
    "cms:migrate-content": "env-cmd -f ./cms/storyblok.env storyblok-migrate --content-migrations",
    "cms:backup": "env-cmd -f ./cms/storyblok.env storyblok-backup --components --stories",
    "cms:restore": "env-cmd -f ./cms/storyblok.env node ./cms/restore.js",
    "cms:sync": "env-cmd -f ./cms/storyblok.env node ./cms/sync.js"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "@vue/test-utils": "^1.1.3",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.5.1",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^6.2.1",
    "jest": "^25.5.4",
    "jest-transform-graphql": "^2.1.0",
    "nodemon": "^2.0.7",
    "storyblok": "^2.8.0",
    "storyblok-migrate": "^0.3.4",
    "vue-jest": "^3.0.7",
  },
  "moduleRoots": [
  ]
}
